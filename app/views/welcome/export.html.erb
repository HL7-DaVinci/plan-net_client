<section class='container'>
	<% if @client %>
		<p class="text-success m-auto client-connected" style='text-align: center'>
			Exported from  <%= cookies[:server_url] -%>
		</p>
		<table class="table table-dark md-12">
		<tr>
		<td>Request</td>
		<td><%=@request%></td>
		<tr>
		<tr>
		<td>Requires Token</td>
		<td><%=@requiresToken%></td>
		<tr>
		</table>
		<table class="table table-dark md-12">
		<% @outputs.each do |output| %>
		<tr>
		<td><%= output["type"] %> </td>
		<td><%= output["url"] %> </td>
		</tr>		
		<% end %>
		</table>
	<%else%>
		<p class="text-danger m-auto" style='text-align: center'>No Connection Established</p>
	<% end %>
</section>
